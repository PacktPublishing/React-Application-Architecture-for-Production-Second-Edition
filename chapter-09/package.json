{
  "name": "aideas",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "preview": "dotenv -e .env -- sh -c 'PORT=5173 react-router-serve ./build/server/index.js'",
    "analyze": "npx vite-bundle-visualizer",
    "typecheck": "react-router typegen && tsc",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint-staged": "lint-staged --config infra/lint-staged.config.js",
    "pre-commit": "npm run lint-staged",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate:openapi": "dotenv -e .env -- openapi-ts"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@hookform/resolvers": "^5.2.2",
    "@react-router/node": "^7.13.0",
    "@react-router/serve": "^7.13.0",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-virtual": "^3.13.18",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "i18next": "^25.8.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-fetch-backend": "^7.0.0",
    "isbot": "^5.1.34",
    "isomorphic-dompurify": "^2.35.0",
    "lucide-react": "^0.563.0",
    "nuqs": "^2.8.7",
    "pretty-cache-header": "^1.0.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.71.1",
    "react-i18next": "^16.5.4",
    "react-router": "^7.13.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-html": "^16.0.1",
    "remix-i18next": "^7.4.2",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@hey-api/openapi-ts": "^0.91.1",
    "@react-router/dev": "^7.13.0",
    "@storybook/addon-docs": "^10.2.3",
    "@storybook/react-vite": "^10.2.3",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-check-file": "^3.3.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-storybook": "^10.2.3",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "storybook": "^10.2.3",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
